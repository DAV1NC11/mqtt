<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashMobileVM" type="com.davncsapplication.app.modules.splashmobile.data.viewmodel.SplashMobileVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSplashmobile"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStyle">
        <ImageView android:id="@+id/imageLogo"
            android:layout_gravity="center"
            android:layout_height="@dimen/_332pxh"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="@dimen/_332pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_rectangle33}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle33"/>
    </FrameLayout>
</layout>
